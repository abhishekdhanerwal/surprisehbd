{
  "homepage": "http://abhishekdhanerwal.github.io/surprisehbd",
  "name": "drpl-frontend",
  "version": "1.0.0",
  "description": "drpl frontend",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node tools/build.js",
    "prebuild:preprod": "npm-run-all clean-dist lint",
    "build:preprod": "babel-node tools/build.preProd.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "abhishek",
  "license": "MIT",
